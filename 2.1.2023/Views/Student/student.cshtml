
@{
    ViewBag.Title = "student";
}

<h1>student</h1>

<table  class="table alert-danger" >
    <tr>
        <th>Name </th>
        <th>Id </th>
        <th>Major </th>
        <th>Facility </th>

    </tr>

    @foreach (var item in Model)
    {

      <tr>
          <td>  @item.Name </td>
          <td>  @item.Id </td>
          <td>  @item.Major </td>
          <td>  @item.Faculity </td>

      </tr> 

    }
    </table>